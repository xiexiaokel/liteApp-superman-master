/* pages/prize/prize.wxss */
.prize-tab{
  width:690rpx;
  height:80rpx;
  margin:20rpx auto;
  border-radius: 40rpx;
  overflow:hidden;
}
.tab-item{
  display:flex;
  flex:1;
  justify-content: center;
  align-items: center;
  height:80rpx;
  line-height: 80rpx;
  background:#fff;
}
.default{
  color:#3963e0;
  display:block;
  width:100%;
  text-align: center;
}
.active{
  background:#3963e0;
  color:#fff;
}
.list-content{
  padding:0 30rpx;
}